{
  "expo": {
    "name": "restauranteeo32",
    "slug": "restauranteeo32",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/PedidosYaIcon.png",
    "scheme": "restauranteeo32",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/PedidosYaIcon.png",
        "backgroundImage": "./assets/images/PedidosYaIcon.png",
        "monochromeImage": "./assets/images/PedidosYaIcon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.xwlspider.restauranteeo32",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/PedidosYaIcon.png"
    },
    "plugins": [
      "expo-router",
      "expo-updates",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#E89090"
        }
         
      ]
    ],
    "updates": {
      "url": "https://u.expo.dev/98506dae-5969-48e2-b09d-fc222f633c14",
      "enabled": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "98506dae-5969-48e2-b09d-fc222f633c14"
      }
    }
  }
}